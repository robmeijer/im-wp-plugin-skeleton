{
    "name": "immediate/im-wp-plugin",
    "description": "A WordPress plugin base package",
    "keywords": ["wordpress"],
    "type": "library",
    "license": "Proprietary",
    "autoload": {
        "psr-4": {
            "IM\\Fabric\\Package\\Plugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IM\\Fabric\\Package\\Plugin\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.immediate.co.uk:WCP-Packages/im-fabric-wordpress.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "league/container": "^2.4",
        "immediate/im-fabric-wordpress": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
